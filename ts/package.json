{
    "name": "@anz-bank/sysl",
    "version": "0.21.0",
    "description": "Sysl (pronounced \"sizzle\") is a open source system specification language.",
    "author": "ANZ Bank",
    "publisher": "anz-bank",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/anz-bank/sysl"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "specification",
        "diagrams"
    ],
    "files": [
        "README.md",
        "dist"
    ],
    "exports": {
        ".": "./dist/index.js",
        "./common": "./dist/common/index.js",
        "./model": "./dist/model/index.js",
        "./pbModel": "./dist/pbModel/index.js"
    },
    "types": "dist/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "dist/index.d.ts"
            ],
            "common": [
                "dist/common/index.d.ts"
            ],
            "model": [
                "dist/model/index.d.ts"
            ],
            "pbModel": [
                "dist/pbModel/index.d.ts"
            ]
        }
    },
    "scripts": {
        "prepublish": "yarn build",
        "build": "tsc",
        "build:watch": "tsc -w",
        "test": "jest",
        "test:watch": "jest --watch",
        "generate": "sysl pb --mode=pb test/all.sysl > test/all.pb && sysl pb --mode=json test/all.sysl > test/all.json",
        "prettier:write": "yarn prettier --write .",
        "prettier:check": "yarn prettier --check .",
        "analyse": "dpdm ./src/index.ts"
    },
    "dependencies": {
        "promisify-child-process": "^4",
        "reflect-metadata": "^0",
        "typedjson": "^1"
    },
    "devDependencies": {
        "@types/jest": "^29",
        "jest": "^29",
        "ts-jest": "^29",
        "jest-extended": "^3",
        "dpdm": "^3",
        "prettier": "^2",
        "ts-node": "^10",
        "typescript": "^4"
    }
}
