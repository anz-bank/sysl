{
    "name": "@anz-bank/sysl",
    "version": "0.6.0",
    "description": "Sysl (pronounced \"sizzle\") is a open source system specification language.",
    "author": "ANZ Bank",
    "publisher": "anz-bank",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/anz-bank/sysl"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "specification",
        "diagrams"
    ],
    "files": [
        "README.md",
        "dist/**/!(*.test).js",
        "dist/**/!(*.test).d.ts"
    ],
    "exports": {
        ".": "./dist/index.js",
        "model": "./dist/model/index.js",
        "pbModel": "./dist/pbModel/index.js"
    },
    "types": "dist/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "dist/index.d.ts"
            ],
            "model": [
                "dist/model/index.d.ts"
            ],
            "pbModel": [
                "dist/pbModel/index.d.ts"
            ]
        }
    },
    "scripts": {
        "prepublish": "yarn build",
        "build": "tsc",
        "test": "jest",
        "test:watch": "jest --watch",
        "prettier:write": "yarn prettier --write .",
        "prettier:check": "yarn prettier --check ."
    },
    "dependencies": {
        "promisify-child-process": "^4",
        "reflect-metadata": "^0",
        "typedjson": "^1"
    },
    "devDependencies": {
        "@types/jest": "^27",
        "jest": "^27",
        "jest-extended": "^2",
        "prettier": "^2.7.1",
        "ts-jest": "^27",
        "ts-node": "^10",
        "typescript": "^4"
    }
}
