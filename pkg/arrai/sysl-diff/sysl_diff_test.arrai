let rec addChange = \model \change
    model :> cond . {
        {...}: . => (. +> (:change)),
        (...): . :> (. => (. +> (:change))),
    }
;
(
   additions: test((), model) = addChange(model, 'added'),
   deletions: test(model, ()) = addChange(model, 'deleted'),
   modified: test(model, modified) = //{./test_diff_out},
)