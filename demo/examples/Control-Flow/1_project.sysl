Foo:
  !type ErrorResponse:
    status <: string

  !type ResourceNotFoundError:
    status <: string 
  /todos:
    /{id<:int}:
      GET:
        # Here we can use if (or IF) and specify conditional returns based on the https status codes
        if notfound:
          return 404 <: ResourceNotFoundError
        else if failed:
          return 500 <: ErrorResponse
        else:    
          return 200 <: Todo 
