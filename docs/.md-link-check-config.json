{
  "replacementPatterns": [
    {
      "pattern": "^(/docs/.*)",
      "replacement": "$1.md"
    },
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://docs.github.com/", "https://guides.github.com/"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ]
}
