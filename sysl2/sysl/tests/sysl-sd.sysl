# ./sd -app Project -output "sysl-sd-%(epname)".png -modules ./test/sysl-sd.sysl
Database[~db]:
    QueryUser (user_id):
        Return User

Api:
    /users/{user_id<:int}/profile:
        GET:
                Database <- QueryUser(user_id)
                Return UserProfile

WebFrontend:
    RequestProfile:
        Api <- GET /users/{user_id}/profile
        Return Profile Page

Project [seqtitle="Profile", blackboxes=[["你好", "bb"], ["aaa", "bbb"]]]:
    _ [ddd="testddd", blackboxes=[["你好!", "cc"], ["eee", "fff"]]]:
        WebFrontend <- RequestProfile
